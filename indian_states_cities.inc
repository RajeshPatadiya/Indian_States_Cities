/**
 * Batch Operation Callback
 */
function _indian_states_cities_taxo_creation_process($vid, $terms, &$context) {
  if (!isset($context['sandbox']['progress'])) {
    $context['sandbox']['progress'] = 0;
    $context['sandbox']['current_node'] = 0;
    $context['sandbox']['max'] = count($terms);
    $context['sandbox']['unprocessed_terms'] = $terms;
  }
  // Fetch unprocessed terms from sandbox.
  $unprocessed_terms = $context['sandbox']['unprocessed_terms'];

  // Proccessing only 1 state per request.
  // Initialise empty parent-children variables.
  $parent = NULL;
  $children = array();

  // Get parent.
  $parent = key($unprocessed_terms);

  // Get parent's children.
  $children = array_shift($unprocessed_terms);

  if (isset($parent)) {
    // Create the parent term.
    taxonomy_term_save((object) array(
      'name' => $parent,
      'vid' => $vid,
    ));
    $context['results'][] = $parent;

    // Get the parent term's ID.
    $tid = db_query("SELECT tid FROM {taxonomy_term_data} WHERE vid = :vid ORDER BY tid DESC LIMIT 1", array(
      ':vid' => $vid,
    ))->fetchField();
    if (!empty($children)) {
      foreach ($children as $child_term) {
        // Create the child term.
        taxonomy_term_save((object) array(
          'name' => $child_term,
          'vid' => $vid,
          'parent' => array($tid),
        ));
        $context['results'][] = $child_term;
      }
    }
  }
  // Save the rest of unprocessed terms.
  $context['sandbox']['unprocessed_terms'] = $unprocessed_terms;

  // Display current processing state message.
  $context['message'] = t('Creating: %state and its cities.', array('%state' => $parent));

  // Update progress.
  $context['sandbox']['progress']++;

  // Inform the batch engine that we are not finished,
  // and provide an estimation of the completion level we reached.
  if ($context['sandbox']['progress'] != $context['sandbox']['max']) {
    $context['finished'] = $context['sandbox']['progress'] / $context['sandbox']['max'];
  }
}

/**
 * Batch 'finished' callback
 */
function _indian_states_cities_taxo_creation_finished($success, $results, $operations) {
  if ($success) {
    // Show the results.
    $message = t('@count terms successfully created:', array('@count' => count($results)));
    $message .= theme('item_list', array('items' => $results));
    drupal_set_message($message);
  }
  else {
    // An error occurred.
    // $operations contains the operations that remained unprocessed.
    $error_operation = reset($operations);
    $message = t('An error occurred while processing %error_operation with arguments: @arguments', array('%error_operation' => $error_operation[0], '@arguments' => print_r($error_operation[1], TRUE)));
    drupal_set_message($message, 'error');
  }
}
